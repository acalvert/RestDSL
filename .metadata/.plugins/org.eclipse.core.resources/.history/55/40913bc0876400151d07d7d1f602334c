int main() {

	int i_0, j_0, array[10][20];

	for(int i_1 = 0; i_1 < 10; i_1 += 3) {
		for(int j_1 = 0; j_1 < 20; j_1 += 2) {
			for(int i = i_1; i < i_1 + 3 && i < 10; i++) { /*<<<<< 5,1,10,1,3,2,pass*/
				for(int j = j_1; i < j_1 + 2 && j < 20; j++) {
					array[i][j] = 0;
				}
			}
		}
	}

	return 0;
}
