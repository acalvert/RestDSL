int main() {

	int array[10][20][30][4];

	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 20; j++) { /*<<<<< 6,1,13,4,2,2,pass*/
			for (int k = 0; k < 30; k++) {
				for (int m = 0; m < 4; m++) {
					array[i][j][k][m] = 0;
				}
			}
		}
	}
	
	for(int i_0 = 0; i_0 < 10; i += 3) {
		for(int j_0 = 0; j_0 < 20; j_0 += 2) {
			for(int i = i_0; i < i_0 + 3 && i < 10; i++) { /*<<<<< 5,1,12,1,2,2,pass*/
				for(int j = j_0; i < j_0 + 2 && j < 20; j++) {
					for (int k = 0; k < 30; k++) {
						array[i][j][k] = 0;
					}
				}
			}
		}
	}

	return 0;
}
